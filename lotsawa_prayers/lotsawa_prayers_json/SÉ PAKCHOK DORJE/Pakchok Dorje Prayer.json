  "title": {
    "bo": "༄༅། །སྲས་འཕགས་མཆོག་རྡོ་རྗེ་ལ་གསོལ་འདེབས་བཞུགས།"
  },
  "text": [
    {
      "bo": "ན་མོ་གུ་རུ།"
    },
    {
      "bo": "རིག་སྟོང་འོད་གསལ་བདེ་ཆེན་ཕོ་བྲང་ནས།    །"
    },
    {
      "bo": "རང་བྱུང་རིག་པའི་རྩལ་རྫོགས་ཆོས་ཀྱི་སྐུ།    །"
    },
    {
      "bo": "ཀུན་བཟང་ཁྱབ་བདག་བླ་མར་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "ཀ་དག་ཁྲེགས་ཆོད་རྟོགས་པར་བྱིན་གྱིས་རློབས།    །"
    },
    {
      "bo": "རང་མདངས་འགགས་མེད་རང་གསལ་གཞལ་ཡས་ནས།    །"
    },
    {
      "bo": "ལྷུན་གྲུབ་ལོངས་སྐུའི་བླ་མར་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "འོད་གསལ་སྣང་བཞིའི་ལམ་མཆོག་མཐར་ཕྱིན་ནས།    །"
    },
    {
      "bo": "འཇའ་ལུས་འཕོ་ཆེན་འགྲུབ་པར་བྱིན་གྱིས་རློབས།    །"
    },
    {
      "bo": "རིག་རྩལ་ཐུགས་རྗེ་སྤྲུལ་སྐུའི་ཞིང་ཁམས་ནས།    །"
    },
    {
      "bo": "སྤྲུལ་སྐུ་ཀུན་བཟང་འཕགས་མཆོག་རྡོ་རྗེ་ལ།    །"
    },
    {
      "bo": "དྲན་རིག་རྒྱུན་ཆད་མེད་པས་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "འགྲོ་དོན་དཔག་མེད་འབྱུང་བར་བྱིན་གྱིས་རློབས།    །"
    },
    {
      "bo": "སྐྱབས་ཀུན་སྙིང་པོ་རང་རིག་བླ་མར་ཤེས།    །"
    },
    {
      "bo": "སྐུ་གསུམ་ལྷུན་གྲུབ་གཞོན་ནུ་བུམ་སྐུའི་སྦུབས།    །"
    },
    {
      "bo": "གཞོམ་མེད་རྡོ་རྗེའི་ངོ་བོར་མངོན་བྱང་ཆུབ།    །"
    },
    {
      "bo": "སྲིད་གསུམ་འཁོར་བ་དོང་ནས་སྤྲུགས་པར་ཤོག  །"
    }
  ]
}