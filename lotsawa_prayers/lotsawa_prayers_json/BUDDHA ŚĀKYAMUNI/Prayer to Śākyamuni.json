  "title": {
    "bo": "༄༅། །ཇོ་བོ་རིན་པོ་ཆེའི་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "ཉོན་མོངས་བདུད་ཀྱི་སྡེ་ཚོགས་བཅོམ༔"
    },
    {
      "bo": "བསོད་ནམས་ཡེ་ཤེས་ཕུང་པོར་ལྡན༔"
    },
    {
      "bo": "སྲིད་དང་ཞི་བའི་མཐའ་ལས་འདས༔"
    },
    {
      "bo": "ཤཱཀྱའི་ཏོག་ལ་གསོལ་བ་འདེབས༔"
    },
    {
      "bo": "འཇིགས་བརྒྱད་ཞི་བར་བྱིན་གྱིས་རློབས༔"
    },
    {
      "bo": "སྐྱེ་དང་ཚེ་རབས་ཐམས་ཅད་དུ༔"
    },
    {
      "bo": "སླར་ཡང་ལྷག་པའི་ལུས་ཐོབ་ནས༔"
    },
    {
      "bo": "འགྲོ་རྣམས་སྡུག་བསྔལ་ལས་གྲོལ་བའི༔"
    },
    {
      "bo": "དེད་དཔོན་མཆོག་ཏུ་བདག་གྱུར་ཤོག༔"
    }
  ]
}