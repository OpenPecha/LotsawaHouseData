  "title": {
    "bo": "༄༅། །ཆོས་ཀྱི་རྒྱལ་པོ་འཁོར་བཅས་ལ་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "ཆོས་ཀྱི་རྒྱལ་པོ་སངས་རྒྱས་བཅོམ་ལྡན་འདས། །"
    },
    {
      "bo": "བྱང་ཆུབ་སེམས་དཔའ་ཉེ་བའི་སྲས་ཆེན་བརྒྱད། །"
    },
    {
      "bo": "འཕགས་པའི་གནས་བརྟན་ཆེན་པོ་བཅུ་དྲུག་གིས། །"
    },
    {
      "bo": "བདག་ལ་དགོངས་ཤིང་བྱིན་གྱིས་བརླབ་པར་མཛོད། །"
    },
    {
      "bo": "བདག་ནི་ནད་གདོན་གནོད་པ་ཀུན་ལས་གྲོལ། །"
    },
    {
      "bo": "བསྟན་དང་འགྲོ་ལ་ཕན་པའི་དོན་ཆེན་འགྲུབ། །"
    },
    {
      "bo": "ཉམས་དང་རྟོགས་པའི་ཡོན་ཏན་གོང་འཕེལ་ནས། །"
    },
    {
      "bo": "བདེ་བའི་གཏན་སྲིད་ཟིན་པར་བྱིན་གྱིས་རློབས། །"
    }
  ]
}