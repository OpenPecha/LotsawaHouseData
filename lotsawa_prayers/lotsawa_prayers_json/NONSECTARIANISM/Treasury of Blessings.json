    },
    {
      "bo": "བྱང་ཆུབ་སེམས་དཔའ་མཁན་ཆེན་ཞི་བ་འཚོ། །"
    },
    {
      "bo": "གངས་ཅན་བཀའ་དྲིན་མཉམ་མེད་ཁྲི་སྲོང་རྗེ། །"
    },
    {
      "bo": "བསྟན་པའི་སྲོལ་འབྱེད་གསུམ་ལ་གསོལ་བ་འདེབས། །"
    },
    {
      "bo": "བི་མ་ཐོན་མི་སྲོང་བཙན་བཻ་རོ་དང་། །"
    },
    {
      "bo": "རྨ་གཉགས་ཀ་ཅོག་རྒྱལ་སྲས་མཆོག་གྲུབ་རྒྱལ། །"
    },
    {
      "bo": "མཚོ་རྒྱལ་ནམ་སྙིང་ལ་སོགས་རྗེ་འབངས་ལ། །"
    },
    {
      "bo": "བཀའ་དྲིན་དྲན་པའི་ངང་ནས་གསོལ་བ་འདེབས། །"
    },
    {
