  "title": {
    "bo": "༄༅། །བསིལ་གློག་ལས་བསྒྲགས་ལྟར་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "ཁྱབ་བདག་འཇམ་པའི་རྡོ་རྗེར། །"
    },
    {
      "bo": "གསོལ་བ་སྙིང་ནས་འདེབས་སོ། །"
    },
    {
      "bo": "བྱིན་རླབས་ཡེ་ཤེས་མཆོག་གིས། །"
    },
    {
      "bo": "བདག་རྒྱུད་གང་བར་མཛོད་ཅིག །"
    }
  ]
}