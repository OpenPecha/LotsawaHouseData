  "title": {
    "bo": "༄༅། །འཇམ་དཔལ་གསོལ་འདེབས་བཞུགས༔"
  },
  "text": [
    {
      "bo": "ཨེ་མ་ཧོ༔"
    },
    {
      "bo": "སྤྲོས་བྲལ་དོན་གྱི་འོག་མིན་ན༔"
    },
    {
      "bo": "ཆོས་སྐུའི་སྟོན་པ་ཀུན་བཟང་ལ༔"
    },
    {
      "bo": "གསོལ་བ་འདེབས་སོ་བྱིན་གྱིས་རློབས༔"
    },
    {
      "bo": "མཁྱེན་རབ་ཐོགས་མེད་དངོས་གྲུབ་རྩོལ༔"
    },
    {
      "bo": "འགགས་མེད་ལྔ་ལྡན་ཆེན་པོའི་ཞིང་༔"
    },
    {
      "bo": "ལོངས་སྐུའི་སྟོན་པ་རིགས་ལྔ་ལ༔"
    },
    {
      "bo": "གསོལ་བ་འདེབས་སོ་བྱིན་གྱིས་རློབས༔"
    },
    {
      "bo": "མཁྱེན་རབ་ཐོགས་མེད་དངོས་གྲུབ་རྩོལ༔"
    },
    {
      "bo": "ཤར་ཕྱོགས་རི་བོ་རྩེ་ལྔའི་གནས༔"
    },
    {
      "bo": "སྤྲུལ་སྐུ་འཇམ་དཔལ་རིགས་ལྔ་ལ༔"
    },
    {
      "bo": "གསོལ་བ་འདེབས་སོ་བྱིན་གྱིས་རློབས༔"
    },
    {
      "bo": "མཁྱེན་རབ་ཐོགས་མེད་དངོས་གྲུབ་རྩོལ༔"
    },
    {
      "bo": "དགའ་རབ་ཤྲི་སེང་བཤེས་གཉེན་དང་༔"
    },
    {
      "bo": "ཛྙཱ་ན་སཱུ་དྲ་བི་མ་ལར༔"
    },
    {
      "bo": "གསོལ་བ་འདེབས་སོ་བྱིན་གྱིས་རློབས༔"
    },
    {
      "bo": "མཁྱེན་རབ་ཐོགས་མེད་དངོས་གྲུབ་རྩོལ༔"
    },
    {
      "bo": "བརྫུས་སྐྱེས་སློབ་དཔོན་སཾ་བྷ་ཝ༔"
    },
    {
      "bo": "མཚོ་རྒྱལ་རྗེ་འབངས་རིག་འཛིན་ལ༔"
    },
    {
      "bo": "གསོལ་བ་འདེབས་སོ་བྱིན་གྱིས་རློབས༔"
    },
    {
      "bo": "མཁྱེན་རབ་ཐོགས་མེད་དངོས་གྲུབ་རྩོལ༔"
    },
    {
      "bo": "ལྷ་སྲས་ཆེན་པོ་ཕྲིན་ལས་གླིང་༔"
    },
    {
      "bo": "བརྒྱུད་གསུམ་རིག་འཛིན་བླ་མ་ལ༔"
    },
    {
      "bo": "གསོལ་བ་འདེབས་སོ་བྱིན་གྱིས་རློབས༔"
    },
    {
      "bo": "མཁྱེན་རབ་ཐོགས་མེད་དངོས་གྲུབ་རྩོལ༔"
    },
    {
      "bo": "རྩ་གསུམ་ལྷ་ཚོགས་ཐམས་ཅད་ལ༔"
    },
    {
      "bo": "གུས་པས་གསོལ་བཏབ་མཐུ་བྱིན་གྱིས༔"
    },
    {
      "bo": "ཆོས་ཀྱི་མཐུན་རྐྱེན་ཡིད་བཞིན་འགྲུབ༔"
    },
    {
      "bo": "འགལ་རྐྱེན་བར་ཆད་མ་ལུས་སེལ༔"
    },
    {
      "bo": "མཆོག་དང་ཐུན་མོང་དངོས་གྲུབ་རྣམས༔"
    },
    {
      "bo": "ཚེ་འདིར་ཐོབ་ནས་འབད་མེད་དུ༔"
    },
    {
      "bo": "ས་ལམ་ཡོན་ཏན་མཐའ་རུ་ཕྱིན༔"
    },
    {
      "bo": "རྡོ་རྗེ་འཆང་དབང་བླ་མ་དང་༔"
    },
    {
      "bo": "དབྱེར་མེད་མགོན་པོ་འཇམ་དཔལ་དབྱངས༔"
    },
    {
      "bo": "གོ་འཕང་བདེ་བླག་ཐོབ་པར་ཤོག༔"
    }
  ]
}