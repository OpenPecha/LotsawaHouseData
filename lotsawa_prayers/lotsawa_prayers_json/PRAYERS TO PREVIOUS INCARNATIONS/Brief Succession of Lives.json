  "copyright": "https://creativecommons.org/licenses/by-nc/4.0/",
  "title": {
    "bo": "༄༅། །སྐྱེ་རབས་བསྡུས་པ་བཞུགས།"
  },
  "text": [
    {
      "bo": "ཕྱི་ལྟར་སློབ་དཔོན་འཇམ་དཔལ་བཤེས་གཉེན་དང༌། །"
    },
    {
      "bo": "ནང་ལྟར་རིག་འཛིན་ཆེན་པོ་ཧཱུྃ་ཀ་ར། །"
    },
    {
      "bo": "གསང་བ་པཎ་ཆེན་བི་མ་མི་ཏྲའི་ཞབས། །"
    },
    {
      "bo": "ཡང་གསང་ཆོས་ཀྱི་རྒྱལ་པོ་ཀློང་ཆེན་པ། །"
    },
    {
      "bo": "རྡོ་རྗེ་གཟི་བརྗིད་རྩལ་ལ་གསོལ་བ་འདེབས། །"
    },
    {
      "bo": "ཤེས་རབ་ཆེན་པོས་ཆོས་ཀྱི་དེ་ཉིད་གཟིགས། །"
    },
    {
      "bo": "སྙིང་རྗེ་ཆེན་པོས་འགྲོ་རྣམས་རྗེས་སུ་འཛིན། །"
    },
    {
      "bo": "དཔལ་ལྡན་བླ་མས་རྗེས་བཟུང་མཁྱེན་བརྩེའི་གཏེར། །"
    },
    {
      "bo": "ངག་གི་དབང་པོའི་ཞབས་ལ་གསོལ་བ་འདེབས། །"
    },
    {
      "bo": "འདི་དང་ཕྱི་མ་བར་དོ་ཐམས་ཅད་དུ། །"
    },
    {
      "bo": "འབྲལ་མེད་བརྩེ་བས་རྗེས་བཟུང་སྨིན་གྲོལ་གྱི། །"
    },
    {
      "bo": "གསང་ཆེན་བདུད་རྩི་ཐོབ་ནས་འགྲོ་ཀུན་གྱི། །"
    },
    {
      "bo": "རྟག་ཏུ་ཕན་བདེ་འགྲུབ་པར་བྱིན་གྱིས་རློབས། །"
    }
  ]
}