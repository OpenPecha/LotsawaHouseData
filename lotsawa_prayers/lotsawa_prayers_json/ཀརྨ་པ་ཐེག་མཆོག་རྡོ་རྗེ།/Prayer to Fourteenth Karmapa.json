  "title": {
    "bo": "༄༅། །ཀརྨ་པ་ཐེག་མཆོག་རྡོ་རྗེའི་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "སངས་རྒྱས་ཀུན་གྱི་བདག་ཉིད། །"
    },
    {
      "bo": "རྒྱལ་དབང་ཀརྨ་པ་ཆེན་པོ། །"
    },
    {
      "bo": "ཐེག་པ་མཆོག་གི་རྡོ་རྗེ། །"
    },
    {
      "bo": "ཡེ་ཤེས་སྤྱན་གྱིས་གཟིགས་ཤིག །"
    },
    {
      "bo": "ཐུགས་རྗེས་མྱུར་དུ་ཟུངས་ལ། །"
    },
    {
      "bo": "ཕྱི་ནང་བར་ཆད་ཀུན་སོལ། །"
    },
    {
      "bo": "མཆོག་དང་ཐུན་མོང་དངོས་གྲུབ། །"
    },
    {
      "bo": "ད་ལྟ་ཉིད་དུ་སྩོལ་ཅིག །"
    }
  ]
}