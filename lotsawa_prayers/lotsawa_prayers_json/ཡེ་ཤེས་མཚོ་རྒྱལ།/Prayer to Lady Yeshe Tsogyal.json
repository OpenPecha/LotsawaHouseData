{
  "source": "lotsawahouse",
  "source_url": "https://www.lotsawahouse.org/bo/tibetan-masters/jamyang-khyentse-chokyi-lodro/prayer-to-lady-yeshe-tsogyal",
  "copyright": "https://creativecommons.org/licenses/by-nc/4.0/",
  "title": {
    "bo": "༄༅། །ཨ་མ་ཇོ་མོ་མཚོ་རྒྱལ་གྱི་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "ཨ་མ་ཇོ་མོ་མཚོ་རྒྱལ་ལ་གསོལ་བ་འདེབས་སོ།    །"
    },
    {
      "bo": "རྗེ་བཙུན་རྡོ་རྗེ་རྣལ་འབྱོར་མས་བྱིན་གྱིས་རློབས་ཤིག  །"
    },
    {
      "bo": "མ་གཅིག་བཀའ་དྲིན་ཅན་ལ་གསོལ་བ་འདེབས་སོ།    །"
    },
    {
      "bo": "བདེ་ཆེན་ཡེ་ཤེས་མཚོ་རྒྱལ་གྱིས་བྱིན་གྱིས་རློབས་ཤིག  །"
    },
    {
      "bo": "རྒྱལ་ཡུམ་རྡོ་རྗེ་རྣལ་འབྱོར་མ་ལ་གསོལ་བ་འདེབས་སོ།    །"
    },
    {
      "bo": "དབྱིངས་ཕྱུག་ཡེ་ཤེས་ཌཱ་ཀིས་བྱིན་གྱིས་རློབས་ཤིག  །"
    },
    {
      "bo": "རྗེ་བཙུན་རྡོ་རྗེ་ཕག་མོ་ལ་གསོལ་བ་འདེབས་སོ།    །"
    },
    {
      "bo": "རྗེ་བཙུན་ཨརྱ་ཏཱ་རེས་བྱིན་གྱིས་རློབས་ཤིག  །"
    },
    {
      "bo": "དུས་གསུམ་སངས་རྒྱས་ཐམས་ཅད་ཀྱི་ངོ་བོ།    །"
    },
    {
      "bo": "དབྱིངས་ཡུམ་རིགས་ལྔའི་རྣམ་འཕྲུལ།    །"
    },
    {
      "bo": "མཁར་ཆེན་གཟའ་ཡི་མཚོ་རྒྱལ།    །"
    },
    {
      "bo": "འཕགས་མ་དབྱངས་ཅན་སྤྲུལ་པ།    །"
    },
    {
      "bo": "ཁྱེད་ལ་གསོལ་བ་འདེབས་སོ།    །"
    },
    {
      "bo": "ཌཱ་ཀི་འབུམ་སྡེའི་གཙོ་མོ།    །"
    },
    {
      "bo": "གུ་རུའི་ཐུགས་ཀྱི་གཟུངས་མ།    །"
    },
    {
      "bo": "གསང་ཆེན་བཀའ་ཡི་སྡུད་པོ།    །"
    },
    {
      "bo": "བོད་ཁམས་སྐྱོབས་པའི་ཨ་མ།    །"
    },
    {
      "bo": "མ་ཅིག་དྲིན་མོ་ཆེ་ལ་གསོལ་བ་འདེབས་སོ།    །"
    },
    {
      "bo": "གང་འདུལ་སྐུ་ཡི་སྣང་བས།    །"
    },
    {
      "bo": "བདག་ལ་བྱིན་གྱིས་རློབས་ཤིག  །"
    },
    {
      "bo": "མ་དག་རྩ་ཡི་དྲྭ་བ།    །"
    },
    {
      "bo": "དག་པ་རྡོ་རྗེའི་སྐུར་སྨིན།    །"
    },
    {
      "bo": "ཁྱོད་དང་དབྱེར་མེད་ཤོག་ཅིག  །"
    },
    {
      "bo": "འགྲོ་འོང་རླུང་གི་རྒྱུ་བ།    །"
    },
    {
      "bo": "དྷཱུ་ཏིའི་དབྱིངས་སུ་དེངས་ནས།    །"
    },
    {
      "bo": "མི་ཤིག་ནཱ་དའི་གསུང་མཆོག  །"
    },
    {
      "bo": "ངག་ནི་རྡོ་རྗེའི་གསུང་དབྱངས།    །"
    },
    {
      "bo": "འཕགས་མས་རང་སྐུར་སྨིན་མཛོད།    །"
    },
    {
      "bo": "འཕོ་བཅས་ཐིག་ལེའི་ཚོགས་རྣམས།    །"
    },
    {
      "bo": "གཞོམ་བྲལ་བདེ་ཆེན་སྐུར་སྨིན།    །"
    },
    {
      "bo": "སྒྲིབ་བྲལ་ཆོས་ཀྱི་སྐུ་རུ།    །"
    },
    {
      "bo": "མངོན་སུམ་ཉིད་དུ་སྨིན་མཛོད།    །"
    },
    {
      "bo": "ཀུན་གཞིའི་སྒྲིབ་ཆ་ཕྲ་བའི།    །"
    },
    {
      "bo": "ལས་རླུང་སྲོག་གི་གཡོ་བ།    །"
    },
    {
      "bo": "རྡུལ་བྲལ་རྡོ་རྗེའི་ནམ་མཁར།    །"
    },
    {
      "bo": "མི་གཡོ་མཆོག་ཏུ་བཅིངས་པས།    །"
    },
    {
      "bo": "ངོ་བོ་ཉིད་ཀྱི་སྐུ་རུ།    །"
    },
    {
      "bo": "སྨིན་ནས་དོན་གཉིས་ལྷུན་གྲུབ།    །"
    },
    {
      "bo": "རིག་འཛིན་དྷཱ་ཀིའི་གཙོ་མོ།    །"
    },
    {
      "bo": "བདེ་མཆོག་ཧེ་རུ་ཀ་དཔལ།    །"
    },
    {
      "bo": "བདེ་སྟོང་སྒྱུ་མའི་གར་གྱིས།    །"
    },
    {
      "bo": "ས་གསུམ་ཀུན་ཏུ་ཁྱབ་ཅིང་།    །"
    },
    {
      "bo": "ནམ་མཁར་ཇི་སྲིད་བར་དུ།    །"
    },
    {
      "bo": "སྤྲུལ་པ་དཔག་ཏུ་མེད་པས།    །"
    },
    {
      "bo": "འགྲོ་བའི་དོན་གཉིས་འགྲུབ་པར།    །"
    },
    {
      "bo": "ཨ་མ་ཐུགས་རྗེ་ཅན་གྱིས།    །"
    },
    {
      "bo": "མཁྱེན་བརྩེ་གློག་ལྟར་གྱུར་བས།    །"
    },
    {
      "bo": "བདག་ལ་ཐུགས་རྗེས་གཟིགས་ནས།    །"
    },
    {
      "bo": "སྐལ་ལྡན་སྒྲུབ་པོ་ཚེ་འདིར།    །"
    },
    {
      "bo": "འཆི་མེད་རིག་འཛིན་བརྙེས་པ།"
    },
    {
      "bo": "ཡུམ་ཆེན་ཁྱོད་ཀྱིས་བྱིན་གྱིས་རང་རློབས་ཤིག  །"
    },
    {
      "bo": "མ་གཅིག་བཀའ་དྲིན་ཅན་ཁྱོད་མཁྱེན་ནོ།    །"
    }
  ]
}