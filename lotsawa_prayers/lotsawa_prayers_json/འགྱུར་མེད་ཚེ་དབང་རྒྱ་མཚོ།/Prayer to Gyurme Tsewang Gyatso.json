  "title": {
    "bo": "༄༅། །འགྱུར་མེད་ཚེ་དབང་རྒྱ་མཚོའི་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "གདོད་ནས་འགྱུར༵་མེད༵་བདེ་ཆེན་རྡོ་རྗེའི་ཁམས།    །"
    },
    {
      "bo": "འཕོ་འགྱུར་བྲལ་བའི་ཚེ༵་དབ༵ང་རིག་པ་འཛིན།    །"
    },
    {
      "bo": "གཞན་དབང་དག་པ་ཡོངས་གྲུབ་རྒྱ༵་མཚོ༵འི་གླིང་།    །"
    },
    {
      "bo": "བཞུགས་མཛད་འཁོར་ལོའི་མགོན་པོར་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "འཇམ་དབྱངས་བླ་མས་བྱིན་གྱིས་བརླབས་པ་ལས།    །"
    },
    {
      "bo": "ཟབ་རྒྱས་ཆོས་ཚུལ་ནོར་བུའི་མཛོད་འཆང་ཤིང་།    །"
    },
    {
      "bo": "འཕོ་མེད་ཐིག་ལེ་དབུ་མར་རབ་འཆིང་པ།    །"
    },
    {
      "bo": "ཚུལ་ཁྲིམས་མཚན་ལྡན་བླ་མར་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "ལོ་ཙཱ་ཆེན་པོ་རྣམ་པར་སྣང་མཛད་ཀྱི།    །"
    },
    {
      "bo": "གསང་གསུམ་ཡེ་ཤེས་དྲྭ་བའི་སྒྱུ་འཕྲུལ་གར།    །"
    },
    {
      "bo": "སྙིགས་དུས་འགྲོ་བ་འདུལ་བའི་ཕྲིན་ལས་ཅན།    །"
    },
    {
      "bo": "འཆི་མེད་སྣང་མཛད་རྡོ་རྗེ་གསོལ་བ་འདེབས།།    །།"
    }
  ]
}