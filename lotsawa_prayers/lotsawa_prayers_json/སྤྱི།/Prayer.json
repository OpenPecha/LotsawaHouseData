  "title": {
    "bo": "༄༅། །གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "སྐྱབས་གནས་རྒྱ་མཚོའི་འདུས་ཞལ་གུ་རུ་རྗེ། །"
    },
    {
      "bo": "རིག་འཛིན་མཁའ་འགྲོ་དམ་ཅན་ཚོགས་བཅས་ལ། །"
    },
    {
      "bo": "གདུང་ཤུགས་དྲག་པོས་རྩེ་གཅིག་གསོལ་འདེབས་ན། །"
    },
    {
      "bo": "ཕྱི་ནང་གསང་བའི་བར་ཆད་དབྱིངས་སུ་སོལ། །"
    },
    {
      "bo": "འཇིགས་བརྒྱད་དུས་མིན་འཆི་དང་དགྲ་ལས་རྒྱལ། །"
    },
    {
      "bo": "བསམ་དོན་ཡིད་བཞིན་འགྲུབ་པར་མཛད་དུ་གསོལ། །"
    }
  ]
}