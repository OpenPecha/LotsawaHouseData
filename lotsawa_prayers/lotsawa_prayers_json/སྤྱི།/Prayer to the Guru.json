  "title": {
    "bo": "༄༅། །གུ་རུའི་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "ཨོཾ་སྭ་སྟི་བི་ཛ་ཡནྟུ།"
    },
    {
      "bo": "རང་རིག་བླ་མ་པདྨ་འབྱུང་གནས་དང་། །"
    },
    {
      "bo": "དབྱེར་མེད་དྲིན་ཅན་བླ་མ་ཆོས་ཀྱི་རྗེ། །"
    },
    {
      "bo": "བདག་སོགས་འགྲོ་བ་འདྲེན་པའི་དེད་དཔོན་ཆེ། །"
    },
    {
      "bo": "འདི་ཕྱི་བར་དོ་ཀུན་ཏུ་གསོལ་འདེབས་ན། །"
    },
    {
      "bo": "སྐྱེ་དང་ཚེ་རབས་ཕྲེང་བར་རྗེས་བཟུངས་ནས། །"
    },
    {
      "bo": "རང་གཞན་དོན་གཉིས་འགྲུབ་པར་མཛད་དུ་གསོལ། །"
    }
  ]
}