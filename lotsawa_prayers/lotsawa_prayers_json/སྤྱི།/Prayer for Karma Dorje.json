  "title": {
    "bo": "༄༅། །ཀརྨ་རྡོ་རྗེས་བསྐུལ་བའི་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "སངས་རྒྱས་ཀུན་འདུས་པདྨ་འབྱུང་། །"
    },
    {
      "bo": "ཡི་དམ་འདུས་པ་དྲག་པོ་རྩལ། །"
    },
    {
      "bo": "གསོལ་བ་འདེབས་སོ་བར་ཆད་སོལ། །"
    },
    {
      "bo": "འཕྲལ་ཡུན་ཀུན་ཏུ་དགེ་ལེགས་སྩོལ། །"
    }
  ]
}