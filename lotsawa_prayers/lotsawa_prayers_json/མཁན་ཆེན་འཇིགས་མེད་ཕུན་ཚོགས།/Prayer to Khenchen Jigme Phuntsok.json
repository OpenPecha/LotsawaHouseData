  "title": {
    "bo": "༄༅། །ཆོས་རྗེ་དམ་པ་ཡིད་བཞིན་ནོར་བུའི་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "འཇིགས་མེད་རྩོད་འཆད་རྩོམ་པའི་གཏེར་ལ་དབང་། །"
    },
    {
      "bo": "ཕུན་ཚོགས་བསླབ་པ་གསུམ་གྱི་ཡོན་ཏན་རྫོགས། །"
    },
    {
      "bo": "དཔག་ཡས་ཕན་བདེའི་འབྱུང་གནས་རིན་པོ་ཆེ། །"
    },
    {
      "bo": "འདྲེན་མཆོག་དཔལ་ལྡན་བླ་མར་གསོལ་བ་འདེབས། །"
    }
  ]
}