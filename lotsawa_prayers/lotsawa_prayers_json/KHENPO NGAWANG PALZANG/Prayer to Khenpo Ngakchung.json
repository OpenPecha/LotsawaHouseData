  "title": {
    "bo": "༄༅། །མཁན་ཆེན་ངག་དབང་དཔལ་བཟང་པོའི་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "ན་མོ་གུ་རུ།"
    },
    {
      "bo": "རྩ་གསུམ་འདུས་ཞལ་གུ་རུ་པད་འབྱུང་དང་།    །"
    },
    {
      "bo": "ཀུན་མཁྱེན་རྫོགས་པའི་སངས་རྒྱས་ཀློང་ཆེན་པ།    །"
    },
    {
      "bo": "དབྱེར་མེད་ངག་གི་དབང་ཕྱུག་ལས་འབྲེལ་རྩལ།    །"
    },
    {
      "bo": "འོད་གསལ་རིན་ཆེན་སྙིང་པོར་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "རྟག་ཏུ་བདག་ལ་བྱིན་གྱིས་རློབས་པ་དང་།    །"
    },
    {
      "bo": "དམ་ཚིག་ཉམས་ཆགས་ཉེས་པ་ཀུན་བྱང་ཞིང་།    །"
    },
    {
      "bo": "ཐུགས་རྒྱུད་དགོངས་པ་མངོན་སུམ་འཕོས་པ་དང་།    །"
    },
    {
      "bo": "འཇའ་ལུས་རྡོ་རྗེའི་སྐུ་མཆོག་ཐོབ་པར་ཤོག  །"
    }
  ]
}