  "title": {
    "bo": "༄༅། །གསོལ་འདེབས་དད་པའི་སྤྲིན་ཕུང་ཞེས་བྱ་བ།"
  },
  "text": [
    {
      "bo": "འཇ༵མ་དབྱང༵ས་བླ་མའི་བྱིན་རླབས་སྙིང་ལ་ཞུགས།    །"
    },
    {
      "bo": "ཆོས༵་ཀྱི་བ༵དུད་རྩིས་བློ༵་གྲོ༵ས་པདྨོ་བྱེ།    །"
    },
    {
      "bo": "རིས་མེད་བསྟན་པའི་རྒྱལ་མཚན་བསྒྲེང་ཉིད་ལ།    །"
    },
    {
      "bo": "སྙིང་ནས་གསོལ་འདེབས་བདག་རྒྱུད་བྱིན་གྱིས་རློབས།    །"
    },
    {
      "bo": "གཙུག༵་ལག༵་བལྟ་བའི་རྣམ་དཔྱོད་མཐུ་གྲུབ་ཅིང་།    །"
    },
    {
      "bo": "ལུང༵་རིག༵ས་རྡོ་རྗེའི་གད་མོ་རབ་ཏུ་སྒྲོགས།    །"
    },
    {
      "bo": "ཉི༵་མ༵འི་གཉེན་གྱི་བསྟན་པ་གསལ་བྱེད་ལ།    །"
    },
    {
      "bo": "སྙིང་ནས་གསོལ་འདེབས་བདག་རྒྱུད་བྱིན་གྱིས་རློབས།    །"
    },
    {
      "bo": "སྨྲ༵་བའི༵་དབང་ཕྱུག་འཇམ་དཔལ་དཔའ་བོ་བཞིན།    །"
    },
    {
      "bo": "ལྟ་ངན་རི་དྭགས་ལེགས་བཤད་སེང༵་གེའི༵་སྒྲས།    །"
    },
    {
      "bo": "བཅོམ་པའི་སྙིང་སྟོབས་ཆེན་པོར་གྱུར་པ་གང་།    །"
    },
    {
      "bo": "གཞོམ་མེད་རྡོ་རྗེའི་ཆོས་བདུན་བརྟན་བཞུགས་གསོལ།    །"
    }
  ]
}