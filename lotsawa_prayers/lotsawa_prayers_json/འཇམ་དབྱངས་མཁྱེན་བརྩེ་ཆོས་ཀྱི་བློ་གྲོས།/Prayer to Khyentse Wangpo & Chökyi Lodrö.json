  "copyright": "https://creativecommons.org/licenses/by-nc/4.0/",
  "title": {
    "bo": "༄༅། །འཇམ་དབྱངས་མཁྱེན་བརྩེ་སྐུ་གོང་འོག་གི་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "བཀའ་བབས་བདུན་ལྡན་འཇམ་དབྱངས་མཁྱེན་བརྩེའི་དབང་།    །"
    },
    {
      "bo": "པདྨའི་རྒྱལ་ཚབ་པདྨ་མདོ་སྔགས་གླིང་།    །"
    },
    {
      "bo": "འོད་གསལ་སྤྲུལ་པའི་རྡོ་རྗེའི་ཞབས་པད་མོར།    །"
    },
    {
      "bo": "སྙིང་ནས་གསོལ་བ་འདེབས་སོ་བྱིན་གྱིས་རློབས།    །"
    },
    {
      "bo": "འཇ༵མ་དབྱང༵ས་བླ་མ་གང་དེའི་རྣམ་པར་འཕྲུལ།    །"
    },
    {
      "bo": "བློ༵་གྲོས༵་རྒྱ༵་མཚོ༵་མདོ་སྔགས་ཤིང་རྟ་ཆེ།    །"
    },
    {
      "bo": "བྱང་ཆུབ་སེམས་དཔའ་ཆོས༵་ཀྱི༵་བློ༵་གྲོ༵ས་ལ།    །"
    },
    {
      "bo": "སྙིང་ནས་གསོལ་འདེབས་བདག་རྒྱུད་བྱིན་གྱིས་རློབས།    །"
    },
    {
      "bo": "པ༵དྨའི་ཟབ་གསང་བསྟན་པ་ཕྱོགས་བཅུར་སྤེལ།    །"
    },
    {
      "bo": "ཡེ༵་ཤེས༵་རྡོ༵་རྗེ༵་ཉིན་མོ་བྱེད་པ་ཡི།    །"
    },
    {
      "bo": "ཐུགས་རྒྱུད་དགོངས་པའི་བྱིན་རླབས་འཕོ་བ་དང་།    །"
    },
    {
      "bo": "བསམ་དོན་ཆོས་བཞིན་འགྲུབ་པར་བྱིན་གྱིས་རློབས།    །"
    }
  ]
}