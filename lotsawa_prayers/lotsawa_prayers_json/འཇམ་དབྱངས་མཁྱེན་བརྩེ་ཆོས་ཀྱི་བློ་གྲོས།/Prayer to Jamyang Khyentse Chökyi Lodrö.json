  "copyright": "https://creativecommons.org/licenses/by-nc/4.0/",
  "title": {
    "bo": "༄༅། །དཔལ་མགོན་བླ་མའི་གསོལ་འདེབས་བྱིན་རླབས་མྱུར་སྟེར་ཞེས་བྱ་བ་བཞུགས།"
  },
  "text": [
    {
      "bo": "ཨོཾ་སྭསྟི།"
    },
    {
      "bo": "འཇ༵མ་དཔལ་དབྱང༵ས་སོགས་རིགས་གསུམ་རྒྱལ་ཀུན་གྱི། །"
    },
    {
      "bo": "མཁྱེན་བརྩེ་ནུས་པའི་ཆོས༵་ཀུན་འདུས་པའི་གཟུགས། །"
    },
    {
      "bo": "མཚོ་སྐྱེས་རྡོ་རྗེའི་ཐུགས་ཀྱི༵་འོད་ཟེར་བཅུད། །"
    },
    {
      "bo": "བློ༵་གྲོས༵་སྤོབས་མཐའ་ཡས་ལ་གསོལ་བ་འདེབས། །"
    },
    {
      "bo": "བདེ་གཤེགས་ཀུན་གྱི་གསང་བ་མི་ཟད་དབྱིངས། །"
    },
    {
      "bo": "འཇ༵མ་དཔལ་དབྱང༵ས་སོགས་དཔལ་ཆེན་སྒྲུབ་སྡེ་བརྒྱད། །"
    },
    {
      "bo": "ཀུན་གྱི་ཁྱབ་བདག་རྒྱུ་འབྲས་ཆོས༵་ཀུན་གྱི༵། །"
    },
    {
      "bo": "འབྱུང་གནས་བློ༵་གྲོས༵་རྨད་བྱུང་གསོལ་བ་འདེབས། །"
    },
    {
      "bo": "འཇ༵མ་མཉེན་མཚན་དཔེའི་རྒྱལ་ཀུན་ཡེ་ཤེས་སྐུ། །"
    },
    {
      "bo": "ཟབ་རྒྱས་དབྱང༵ས་སྙན་ཆོས༵་ཀྱི༵ས་འགྲོ་ཀུན་སྒྲོལ། །"
    },
    {
      "bo": "མཁྱེན་བརྩེའི་བློ༵་གྲོས༵་དཔག་ཡས་ཧེ་རུ་ཀ །"
    },
    {
      "bo": "གནས་གསུམ་མཁའ་འགྲོའི་མེས་པོར་གསོལ་བ་འདེབས། །"
    },
    {
      "bo": "རྩ་གསུམ་ལྷ་ཡི་བདེན་པ་རྨད་བྱུང་གིས། །"
    },
    {
      "bo": "དཔལ་ལྡན་བླ་མ་འཆི་མེད་ཡེ་ཤེས་གཟུགས། །"
    },
    {
      "bo": "སྙིང་གི་པདྨོར་རྟག་བརྟན་བཞུགས་པ་དང༌། །"
    },
    {
      "bo": "ཚེ་རབས་ཀུན་ཏུ་རྗེས་སུ་འཛིན་གྱུར་ཅིག  །"
    }
  ]
}