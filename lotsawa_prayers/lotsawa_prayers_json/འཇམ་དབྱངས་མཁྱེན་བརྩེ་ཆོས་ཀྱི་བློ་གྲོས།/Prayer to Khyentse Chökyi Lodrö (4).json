  "copyright": "https://creativecommons.org/licenses/by-nc/4.0/",
  "title": {
    "bo": "༄༅། །རྒྱལ་ཀུན་མཁྱེན་པའི་ཡེ་ཤེས་སོགས་རང་ཉིད་ཀྱི་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "ཨོཾ་སྭསྟི།"
    },
    {
      "bo": "རྒྱལ་ཀུན་མཁྱེན་པའི་ཡེ་ཤེས་གཏེར་ཆེན་པོ།    །"
    },
    {
      "bo": "འཇམ་དབྱངས་བླ་མ་མཁྱེན་དང་བརྩེ་བའི་དབང་།    །"
    },
    {
      "bo": "དེ་ཡི་ཕྲིན་ལས་རྣམ་རོལ་སྤྲུལ་པའི་སྐུ།    །"
    },
    {
      "bo": "བློ་གྲོས་རྒྱ་མཚོའི་ཞབས་ལ་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "མགོན་པོ་མཆོག་གི་གསང་གསུམ་ཡུན་དུ་བརྟན།    །"
    },
    {
      "bo": "སྐྱེ་དང་ཚེ་རབས་ཀུན་ཏུ་རྗེས་བཟུང་ནས།    །"
    },
    {
      "bo": "ཐེག་མཆོག་གསུང་གི་བདུད་རྩིས་འཚོ་བ་དང་།    །"
    },
    {
      "bo": "བྱིན་རླབས་ཡེ་ཤེས་སྙིང་ལ་འཇུག་པར་ཤོག  །"
    }
  ]
}