    },
    {
      "bo": "ཀུན་ལ་དབང་བསྒྱུར་རྟག་པའི་རྒྱལ་མོ་ཆེར།    །"
    },
    {
      "bo": "སྒྲོལ་མ་མྱུར་མ་དཔའ་མོར་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "ཞི་ལས་མ་གཡོས་སྙིང་རྗེ་ཁྲོས་པ་ཡི།    །"
    },
    {
      "bo": "ཁྲོ་གཉེར་གཡོ་དང་ཧཱུྃ་གི་སྒྲ་བསྒྲགས་པས།    །"
    },
    {
      "bo": "བདུད་དང་བགེགས་རྣམས་མ་ལུས་འཇོམས་མཛད་མ།    །"
    },
    {
      "bo": "སྒྲོལ་མ་དྲག་བྱེད་ཆེན་མོར་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "བདེ་བའི་རོས་མྱོས་བཞད་པའི་ང་རོ་དང་།    །"
    },
    {
      "bo": "ཞབས་གཉིས་བརྡབ་པས་འཇིག་རྟེན་བདུན་པོའི་ཚོགས།    །"
    },
    {
      "bo": "རབ་ཏུ་གནོན་ཅིང་ཁམས་གསུམ་ཟིལ་གྱིས་གནོན།    །"
    },
    {
