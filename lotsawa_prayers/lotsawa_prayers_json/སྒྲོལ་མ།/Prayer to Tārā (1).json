    },
    {
      "bo": "རྣམ་མང་ནོར་བུའི་ཆར་ཆེན་འབེབས་པ་ཡིས།    །"
    },
    {
      "bo": "ཕོངས་པའི་སྡུག་བསྔལ་མཐའ་དག་སེལ་མཛད་པའི།    །"
    },
    {
      "bo": "སྦྱིན་པ་མཐར་ཕྱིན་འཕགས་མར་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "འཁོར་བའི་ཉེས་པ་ཆེ་ལས་རབ་གྲོལ་ཞིང་།    །"
    },
    {
      "bo": "ཟག་བྲལ་དགའ་བ་མཆོག་གི་རྩེར་སོན་ཅིང་།    །"
    },
    {
      "bo": "འཕོ་མེད་བདེ་བ་ཆེ་ལ་རྟག་རོལ་པ།    །"
    },
    {
      "bo": "ཚུལ་ཁྲིམས་ཕར་ཕྱིན་དབང་མོར་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "མཁའ་མཉམ་འགྲོ་བའི་དོན་ལ་རབ་བཟོད་ཅིང་།    །"
    },
    {
      "bo": "ཀུན་གྱི་ཉེར་འཚེ་ས་གཞི་ལྟ་བུ་ཡི།    །"
    },
    {
      "bo": "རྟག་ཏུ་ཁྲོ་མེད་སྙིང་རྗེའི་ཡོངས་གང་བ།    །"
    },
    {
      "bo": "བཟོད་པ་དཀའ་ཐུབ་ཞི་མར་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "སྐད་ཅིག་ཙམ་ཡང་དལ་བ་མེད་པ་ཡི།    །"
    },
    {
      "bo": "འགྲོ་བ་སྒྲོལ་ལ་རབ་ཏུ་བརྩོན་པ་གང་།    །"
    },
    {
      "bo": "འཁོར་བ་སྟོང་པར་མ་གྱུར་དེ་སྲིད་དུ།    །"
    },
    {
      "bo": "བརྩོན་འགྲུས་འགྲོ་སྒྲོལ་མ་ལ་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "མི་གཡོ་ཏིང་འཛིན་རྒྱ་མཚོ་རབ་མཉེས་ཤིང་།    །"
    },
    {
      "bo": "རྣམ་པར་བཅས་པའི་རྟོག་ཚོགས་ཐམས་ཅད་ཞི།    །"
    },
    {
      "bo": "རྣམ་མེད་བདེ་བ་ཆེན་པོའི་རོས་མྱོས་མ།    །"
    },
    {
      "bo": "བསམ་གཏན་གྲུབ་པ་ཆེན་མོར་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "ཟབ་དང་རྒྱ་ཆེའི་ཆོས་ཀུན་མངོན་སུམ་གཟིགས།    །"
    },
    {
      "bo": "ཡེ་ཤེས་རྒྱ་མཚོ་ཟད་པ་མེད་པའི་ཐུགས།    །"
    },
    {
      "bo": "ཡོན་ཏན་རྒྱ་མཚོ་སངས་རྒྱས་ཀུན་དང་མཉམ།    །"
    },
    {
      "bo": "ཤེས་རབ་ཕར་ཕྱིན་སྒྲོལ་མ་གསོལ་བ་འདེབས།    །"
    },
    {
      "bo": "ཁྱེད་ལ་རྟག་ཏུ་དད་པས་གསོལ་འདེབས་མཐུས།    །"
    },
    {
      "bo": "བདག་སོགས་འགྲོ་རྣམས་བར་ཆད་ཀུན་ལས་སྐྱོབས།    །"
    },
    {
      "bo": "སྒྲོལ་མའི་ཐུགས་རྗེས་རྟག་ཏུ་རྗེས་བཟུང་ནས།    །"
    },
    {
      "bo": "དོན་གཉིས་ལྷུན་གྱིས་འགྲུབ་པའི་བཀྲ་ཤིས་ཤོག  །"
    }
