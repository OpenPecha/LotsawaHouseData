  "title": {
    "bo": "༄༅། །སྒྲོལ་མ་ལ་གསོལ་འདེབས་བཞུགས།"
  },
  "text": [
    {
      "bo": "ཤྭྃ༔ ཨ༔"
    },
    {
      "bo": "སྒྱུ་ལུས་དག་པ་རབ་འབྱམས་ཞིང་། །"
    },
    {
      "bo": "སྙིང་དབུས་གཡུ་ལོ་བཀོད་པ་ནས། །"
    },
    {
      "bo": "རིག་པ་སྐུ་གསུམ་རྗེ་བཙུན་མ། །"
    },
    {
      "bo": "རྟོག་ཚོགས་ཏ་རེ་ཉི་ཤུ་གཅིག །"
    },
    {
      "bo": "འདུ་འབྲལ་མེད་པར་གསོལ་བ་འདེབས། །"
    },
    {
      "bo": "གཟུང་འཛིན་ལས་བྱུང་འཇིགས་པ་བརྒྱད། །"
    },
    {
      "bo": "སྐྱོབས་ཤིག་འདོད་དོན་ལྷུན་འགྲུབ་མཛོད། །"
    },
    {
      "bo": "ཚོགས་ཆེན་ས་ལ་ལྷུན་རྫོགས་ཤོག །"
    }
  ]
}