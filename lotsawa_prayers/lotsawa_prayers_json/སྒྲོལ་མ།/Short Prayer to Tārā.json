  "title": {
    "bo": "༄༅། །སྒྲོལ་མའི་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "དུས་གསུམ་རྒྱལ་བ་སྲས་བཅས་སྐྱེད་པའི་ཡུམ། །"
    },
    {
      "bo": "བཅོམ་ལྡན་རྗེ་བཙུན་འཕགས་མ་སྒྲོལ་མ་དང་། །"
    },
    {
      "bo": "དབྱེར་མེད་བླ་མ་སངས་རྒྱས་ཀུན་འདུས་ལ། །"
    },
    {
      "bo": "སྒོ་གསུམ་རྩེ་གཅིག་གུས་པས་གསོལ་བ་འདེབས། །"
    },
    {
      "bo": "འཆི་མེད་རྡོ་རྗེ་བདེ་བའི་ཡེ་ཤེས་ཀྱིས། །"
    },
    {
      "bo": "སྨིན་ཅིང་གྲོལ་ནས་དོན་གཉིས་མྱུར་འགྲུབ་ཤོག །"
    }
  ]
}