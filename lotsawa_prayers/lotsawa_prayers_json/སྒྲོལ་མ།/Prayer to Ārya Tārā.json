  "title": {
    "bo": "༄༅། །འཕགས་མ་སྒྲོལ་མ་ལ་གསོལ་འདེབས་བཞུགས།"
  },
  "text": [
    {
      "bo": "ཨོཾ། འཇིགས་པ་བརྒྱད་སྐྱོབ་མ་ལ་ཕྱག་འཚལ་ལོ། །"
    },
    {
      "bo": "བཀྲ་ཤིས་དཔལ་འབར་མ་ལ་ཕྱག་འཚལ་ལོ། །"
    },
    {
      "bo": "ངན་སོང་སྒོ་འགེགས་མ་ལ་ཕྱག་འཚལ་ལོ། །"
    },
    {
      "bo": "མཐོ་རིས་ལམ་འདྲེན་མ་ལ་ཕྱག་འཚལ་ལོ། །"
    },
    {
      "bo": "རྟག་ཏུ་ཁྱེད་ཀྱིས་སྡོང་གྲོགས་མཛད། །"
    },
    {
      "bo": "ད་དུང་ཐུགས་རྗེས་བསྐྱབ་ཏུ་གསོལ། །"
    }
  ]
}