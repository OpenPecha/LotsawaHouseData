  "title": {
    "bo": "༄༅། །རྗེ་བཙུན་སྒྲོལ་མའི་གསོལ་འདེབས་བསྡུས་པ་བཞུགས།"
  },
  "text": [
    {
      "bo": "ཨོཾ་རྗེ་བཙུན་སྒྲོལ་མ་ཐུགས་རྗེ་ཅན། །"
    },
    {
      "bo": "སྐུ་མདོག་དཀར་མོ་འོད་འཕྲོ་མ། །"
    },
    {
      "bo": "རབ་མཛེས་མདངས་ལྡན་ཡིད་འོང་མ། །"
    },
    {
      "bo": "བདག་ནི་ནད་གདོན་དགྲ་གྱོད་དང་། །"
    },
    {
      "bo": "འཁོར་བ་ངན་སོང་བར་དོ་སོགས། །"
    },
    {
      "bo": "འཇིགས་པ་ཀུན་ལས་བསྐྱབ་ཏུ་གསོལ། །"
    },
    {
      "bo": "ཚེ་རིང་ལུས་སེམས་བདེ་བ་དང་། །"
    },
    {
      "bo": "བསམ་པའི་དོན་ཀུན་འགྲུབ་པ་དང་། །"
    },
    {
      "bo": "ཟས་གོས་ལོངས་སྤྱོད་རྒྱས་གྱུར་ཏེ། །"
    },
    {
      "bo": "རྟག་ཏུ་ཆོས་ལ་སྤྱོད་པར་ཤོག །"
    },
    {
      "bo": "རྒྱལ་ཡུམ་སྒྲོལ་མའི་ཞལ་མཐོང་ནས། །"
    },
    {
      "bo": "རྒྱལ་བའི་སྲས་སུ་བདག་གྱུར་ཅིག །"
    },
    {
      "bo": "རྒྱལ་བའི་དགོངས་པ་རྟོགས་གྱུར་ཏེ། །"
    },
    {
      "bo": "རྒྱལ་བ་རྣམས་དང་སྐལ་མཉམ་ཤོག །"
    }
  ]
}