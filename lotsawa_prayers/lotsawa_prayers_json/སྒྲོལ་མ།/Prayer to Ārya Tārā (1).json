{
  "source": "lotsawahouse",
  "source_url": "https://www.lotsawahouse.org/bo/tibetan-masters/dromton-gyalwe-jungne/tara-prayer",
  "copyright": "https://creativecommons.org/licenses/by-nc/4.0/",
  "title": {
    "bo": "༄༅། །འཕགས་མ་སྒྲོལ་མ་འདོད་གསོལ།"
  },
  "text": [
    {
      "bo": "འཕགས་མ་སྒྲོལ་མ་སྒྲོལ་མཛད་ཡུམ། །"
    },
    {
      "bo": "ཡིད་བཞིན་འཁོར་ལོ་ཚེ་འཕེལ་མ། །"
    },
    {
      "bo": "ལྷ་མོ་ཁྱོད་ལ་གསོལ་བ་ནི། །"
    },
    {
      "bo": "བདག་གི་ཚེ་ཡི་བར་ཆད་དང་། །"
    },
    {
      "bo": "ནད་དང་སྡུག་བསྔལ་ཐམས་ཅད་ལས། །"
    },
    {
      "bo": "ནུས་ལྡན་ཁྱོད་ཀྱིས་བསྐྱབ་ཏུ་གསོལ། །"
    },
    {
      "bo": "མཆོག་དང་ཐུན་མོང་དངོས་གྲུབ་ཀུན། །"
    },
    {
      "bo": "མ་ལུས་བདག་ལ་གནང་བར་མཛོད། །"
    },
    {
      "bo": "འཕགས་མ་ཁྱེད་ལ་དད་བྱེད་པ། །"
    },
    {
      "bo": "དེ་ལ་རྟག་ཏུ་བུ་བཞིན་དགོངས། །"
    },
    {
      "bo": "བདག་ཀྱང་ཁྱོད་ལ་གསོལ་འདེབས་ཀྱི། །"
    },
    {
      "bo": "ཐུགས་རྗེའི་ལྕགས་ཀྱུས་གཟུང་དུ་གསོལ། །"
    },
    {
      "bo": "ལྷ་མོ་སྐུ་མདོག་ཟླ་འདྲའི་མདོག །"
    },
    {
      "bo": "སྒེག་ཅིང་དུལ་བའི་ཆ་བྱད་ཅན། །"
    },
    {
      "bo": "གཟུགས་ལྡན་རིན་ཆེན་རྒྱན་གྱིས་སྤྲས། །"
    },
    {
      "bo": "མཛེས་པ་དར་གྱི་སྨད་གཡོགས་ཅན། །"
    },
    {
      "bo": "པདྨ་ཟླ་བའི་དབུས་གནས་སུ། །"
    },
    {
      "bo": "ཞབས་གཉིས་རྡོ་རྗེ་སྐྱིལ་ཀྲུང་མཛད། །"
    },
    {
      "bo": "ཞལ་གཅིག་ཕྱག་གཉིས་འཛུམ་བག་ཅན། །"
    },
    {
      "bo": "དུས་གསུམ་སངས་རྒྱས་སྐྱེད་པའི་ཡུམ། །"
    },
    {
      "bo": "ལྷ་མོ་ཁྱོད་ལ་རྟག་ཏུ་འདུད། །"
    },
    {
      "bo": "ཁྱེད་ལ་ཆ་ཙམ་བསྟོད་པ་ཡིས། །"
    },
    {
      "bo": "བདག་ཅག་བྱང་ཆུབ་སྒྲུབ་པ་ལ། །"
    },
    {
      "bo": "དུས་འདི་ནས་བཟུང་བྱང་ཆུབ་བར། །"
    },
    {
      "bo": "མི་མཐུན་རྐྱེན་རྣམས་ཞི་གྱུར་ཏེ། །"
    },
    {
      "bo": "མཐུན་རྐྱེན་ཕུན་སུམ་ཚོགས་པར་ཤོག །"
    }
  ]
}