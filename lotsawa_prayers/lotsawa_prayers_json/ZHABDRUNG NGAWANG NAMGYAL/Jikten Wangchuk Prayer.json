  "copyright": "https://creativecommons.org/licenses/by-nc/4.0/",
  "title": {
    "bo": "༄༅། །གསོལ་འདེབས་འཇིག་རྟེན་དབང་ཕྱུག"
  },
  "text": [
    {
      "bo": "འཇིག་རྟེན་དབང་ཕྱུག་ཐུགས་རྗེའི་རླབས་འཕྲེང་དབུས། །"
    },
    {
      "bo": "རྒྱལ་ཀུན་མཁྱེན་བརྩེའི་འདབ་སྟོང་རྒྱས་པ་ལ། །"
    },
    {
      "bo": "ཕན་བདེའི་དྲི་བསུང་ཅི་ཡང་སྤྲོ་བའི་གཉེན། །"
    },
    {
      "bo": "ངག་དབང་རྣམ་རྒྱལ་ཞབས་ལ་གསོལ་བ་འདེབས། །"
    },
    {
      "bo": "མཁྱེན་པས་མ་རིག་མུན་པའི་ཚོགས་འཇོམས་ཤིང༌། །"
    },
    {
      "bo": "བརྩེས་བས་གཞན་དོན་ཡལ་བར་མི་འདོར་བར། །"
    },
    {
      "bo": "ནུས་པས་བདུད་དཔུང་འཇོམས་བྱེད་ལྷ་ཡི་ལྷ། །"
    },
    {
      "bo": "ངག་དབང་རྣམ་རྒྱལ་ཞབས་ལ་གསོལ་བ་འདེབས། །"
    },
    {
      "bo": "ཁམས་གསུམ་སེམས་ཅན་འཁོར་བ་ལས་སྒྲོལ་ཕྱིར། །"
    },
    {
      "bo": "འཆི་མེད་རྡོ་རྗེའི་སྐུ་མཆོག་རབ་བརྙེས་ནས། །"
    },
    {
      "bo": "འཕྲིན་ལས་རྣམ་བཞིས་འགྲོ་བའི་དོན་མཛད་པའི། །"
    },
    {
      "bo": "ཇི་སྲིད་འཚོ་ཞིང་གཞེས་པར་གྱུར་ཅིག་གུ། །"
    }
  ]
}