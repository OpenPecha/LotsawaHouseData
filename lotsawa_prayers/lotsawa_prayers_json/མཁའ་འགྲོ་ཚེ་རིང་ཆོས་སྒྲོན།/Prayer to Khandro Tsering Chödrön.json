  "title": {
    "bo": "༄༅། །མཁའ་འགྲོ་ཚེ་རིང་ཆོས་སྒྲོན་ལ་གསོལ་འདེབས།"
  },
  "text": [
    {
      "bo": "ཨོཾ་སྭ་སྟི།"
    },
    {
      "bo": "ཆོས་སྐུ་ཀུན་བཟང་ཞིང་ན་ཡུམ་ཆེན་མོ། །"
    },
    {
      "bo": "ལོངས་སྐུ་རིགས་ལྔའི་ཞིང་ན་ཝ་ར་ཧི། །"
    },
    {
      "bo": "སྤྲུལ་སྐུ་པད་འབྱུང་ཐབས་མཆོག་ཤེལ་དཀར་མཚོ། །"
    },
    {
      "bo": "མཁྱེན་བརྩེའི་ཌཱཀི་ཚེ་རིང་ཆོས་ཀྱི་སྒྲོན། །"
    },
    {
      "bo": "རྡོ་རྗེ་འཆང་དངོས་ཆོས་ཀྱི་བློ་གྲོས་རྗེ། །"
    },
    {
      "bo": "དཔལ་ལྡན་བླ་མ་དམ་པ་ཡབ་ཡུམ་གྱི། །"
    },
    {
      "bo": "ཐུགས་ཀྱི་བཞེད་དོན་མ་ལུས་ཡོངས་རྫོགས་ནས། །"
    },
    {
      "bo": "བདག་སོགས་འབྲལ་བ་མེད་པར་བྱིན་གྱིས་རློབས། །"
    }
  ]
}